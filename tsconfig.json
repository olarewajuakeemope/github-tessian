{
  "compilerOptions": {
    "module": "es6",
    "target": "es6",
    "allowSyntheticDefaultImports": true,
    "moduleResolution": "node",
    "baseUrl": "./src",
    "sourceMap": true,
    "outDir": "./dist/",
    "jsx": "react",
    "strict": true,
    "lib": [
      "dom",
      "es7",
      "es2017.object"
    ],
    "allowJs": true,
    "strictFunctionTypes": false,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "strictPropertyInitialization": false,
    "esModuleInterop": true
  }
}
